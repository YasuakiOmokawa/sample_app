<div id="board">
  <table>
    <tr id="line">
      <td>検索ワード</td><td>セッション</td><td>クリック数</td><td>CTR</td>
    </tr>
    <%
      cnt = 0
      @search.sort_by{ |a|
        [ -(a.sessions.to_i),
          -(a.adsense_ads_clicks.to_i),
          -(a.adsense_ctr.to_f) ] }.each do |t|
          cnt += 1
    %>
    <tr>
       <td class="row"><%= truncate(t.keyword, :length => 20) %></td>
       <td><%= t.sessions %></td>
       <td><%= t.adsense_ads_clicks %></td>
       <td><%= t.adsense_ctr.to_f.round(1).to_s %>%</td>
    </tr>
    <%
        if cnt >= 10 then break end
      end
    %>
  </table>
</div>
