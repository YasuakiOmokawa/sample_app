<% provide(:title, @user.name) %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
        <% @data.each do |r| %>
          <%= r.date %><p><%= r.pageviews %></p>
        <% end %>
      </h1>
    </section>
  </aside>
</div>
