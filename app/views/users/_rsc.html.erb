<div id="fork">
      <table>
        <tbody><tr id="line">
        <%
          if @title == 'ソーシャル' or @title == 'その他ウェブサイト' then
            head = '参照元'
          else
            head = 'キャンペーン'
          end
        %>
          <td>目標値</td><td><%= head %></td><td>現状値</td><td>差</td>
        </tr>
        <%
          cnt = 0
          @rsc_table.each do |k, v|
            cnt += 1
        %>
        <tr>
          <td><%= v[:good] %></td>
          <td class="row"><%= k %></td>
          <td><%= v[:bad] %></td>
          <td class="gap"><%= v[:gap] %></td>
        </tr>
        <%
          if cnt >= 5 then break end
          end
          # 絞り込み数が5に満たない場合、テーブルを埋める
          if @rsc_table.size <= 5
            tpad_size = 5 - @rsc_table.size
            tpad_size.times {
        %>
        <tr>
          <td>-</td>
          <td class="row">-</td>
          <td>-</td>
          <td>-</td>
        </tr>
        <%
            }
          end
        %>
      </tbody></table>
    </div>
