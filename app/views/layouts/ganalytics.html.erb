<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="ja">
<head>
<meta http-equiv="content-type" content="text/html; charset=utf-8">
<meta http-equiv="content-style-type" content="text/css">
    <title><%= full_title(@title) %></title>
    <%= stylesheet_link_tag "analytics", media: "all" %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= include_gon %>
    <% if @title == 'ホーム' %>
      <%= javascript_include_tag "analytics/useJqplotHome", "data-turbolinks-track" => true %>
    <% else %>
      <%= javascript_include_tag "analytics/useJqplot", "data-turbolinks-track" => true %>
    <% end %>
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>
  </head>
  <body>
    <%= render 'layouts/gaheader' %>
    <% flash.each do |key, value| %>
      <%= content_tag(:div, value, class: "alert alert-#{key}") %>
    <% end %>
    <%= yield %>
    <%= render 'layouts/gafooter' %>
    <%
      if Rails.env.development? then
        [
          params,
          @cond,
          @common,
          @favorite_table,
          @gap_table,
          @gap_table_for_graph,
          @hash_for_graph,
          @value_table_by_days,
          @select_word_for_board,
          @narrow_word,
          @search,
          @in_table,
          @rsc_table,
          @value_table_by_days
          ].each do |t|
    %>
     <%= debug t.inspect %>
    <%
          end
      end
    %>
  </body>
</html>
