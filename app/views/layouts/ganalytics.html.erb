<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="ja">
<head>
<meta http-equiv="content-type" content="text/html; charset=utf-8">
<meta http-equiv="content-style-type" content="text/css">
    <title><%= full_title(@title) %></title>
    <%= stylesheet_link_tag "analytics", media: "all" %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= include_gon %>
    <!-- <%= javascript_include_tag "https://www.google.com/jsapi" %> -->
    <%= render 'layouts/datepicker' %>
    <%= render 'layouts/merger' %>
    <%= render 'layouts/jqplot' %>
    <%= csrf_meta_tags %>
    <%= render 'layouts/shim' %>
  </head>
  <body>
    <%= render 'layouts/gaheader' %>
    <% flash.each do |key, value| %>
      <%= content_tag(:div, value, class: "alert alert-#{key}") %>
    <% end %>
    <%= yield %>
    <%
      if Rails.env.development? then
        [
          params,
          @not_gap_data_for_kitchen,
          @favorite_pages,
          @gap_tables,
          @gap_tables_for_graph,
          @value_table_by_days,
          @select_word_for_board,
          @narrow_word
          ].each do |t|
    %>
     <%= debug t.inspect %>
    <%
          end
      end
    %>
  </body>
</html>
